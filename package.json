  },
  "homepage": "https://houndstooth.cloud",
  "devDependencies": {
    "@bowtie/prettier": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-bowtie": "^1.4.1",
